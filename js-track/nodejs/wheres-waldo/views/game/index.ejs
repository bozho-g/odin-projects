<!DOCTYPE html>
<html lang="en">

<%- include('../partials/head', { title: "Where's Waldo?" }) %>

    <body>
        <%- include('../partials/nav', { isGame: false }) %>

            <div class="container">
                <% pictures.forEach(picture=> { %>
                    <div>
                        <a href="/game/<%= picture.id %>" class="game-image">
                            <h2>
                                <%= picture.name %>
                            </h2>
                            <img src="<%= picture.url %>" alt="<%= picture.name %>">
                        </a>
                    </div>
                    <% }) %>
            </div>
    </body>

</html>